<div class="card-container">
  <div class="card image-container">
    <a class="card-img-top image-box" href='/projects/<%= project.id %>' style="background-image: url(<%= project.image_url %>)">
      <div class="user-detail" id=<%= project.id+'tooltip'%>>
        <div class="user-top">
          <div class="user-image <%= project.owner_id+'image'%>"></div>
          <div class="user-bio <%= project.owner_id+'bio'%>" ></div>
        </div>
        <div class="user-bottom <%= project.owner_id+'stats'%>" >
        </div>
      </div>
      <div class="project-data-container">
        <i class="fas fa-eye"><span> <%= project.views%></span></i>
        <i class="fas fa-user"><span> <%= project.followers%></span></i>
        <i class="fas fa-skull"><span> <%= project.skulls%></span></i>
      </div>
    </a>
    <div class="card-title-container">
      <div class="title">
        <%= project.name %>
      </div>
      <div class="owner" id=<%= project.id%>>
        <div class="owner-image <%= project.owner_id+'image'%>" ></div>
        <div class="owner-name <%= project.owner_id %>"></div>
      </div>
    </div>
  </div>
</div>